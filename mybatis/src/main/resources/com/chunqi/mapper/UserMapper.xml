<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--
    namespace:命名空间
-->

<mapper namespace="com.chunqi.mapper.UserMapper">

    <!--statement-->
    <select id="selectAll" resultType="com.chunqi.pojo.User">
        select * from account;
    </select>
    <select id="selectById" resultType="com.chunqi.pojo.User">
        select * from account where id = #{id};
    </select>
</mapper>